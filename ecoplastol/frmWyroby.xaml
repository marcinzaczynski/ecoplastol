                <DataGridTextColumn 
                    x:Name="opwColumn" 
                    Binding="{Binding opw}" 
                    Header="opw" 
                    Width="60"/>
                <DataGridTextColumn 
                    x:Name="czaswColumn" 
                    Binding="{Binding czasw, StringFormat=\{0:yyyy-MM-dd HH:mm:ss\}}" 
                    Header="czasw" 
                    Width="100"/>
                <DataGridTextColumn 
                    x:Name="opmColumn"
                    Binding="{Binding opm}" 
                    Header="opm" 
                    Width="60"/>
