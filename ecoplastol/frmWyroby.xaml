                                    </DataTemplate>
                                </ts:ComboBoxItemTemplateSelector.DropDownTemplate>
                            </ts:ComboBoxItemTemplateSelector>
                        </ComboBox.ItemTemplateSelector>
                    </ComboBox>
                </DockPanel>
                <ComboBox x:Name="cbbITFICC" Grid.Row="3" Grid.Column="1" FontSize="14" DropDownClosed="CbbITFICC_DropDownClosed" GotFocus="CbbITFICC_GotFocus" LostFocus="CbbITFICC_LostFocus">
