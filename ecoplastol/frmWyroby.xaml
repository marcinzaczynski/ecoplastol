            <!-- panel górny -->
            <RowDefinition Height="40"/>
            <!-- datagrid -->
            <RowDefinition Height="*"/>
            <!-- przyciski dodaj popraw itd -->
            <RowDefinition Height="40"/>
            <!-- textboxy comboboxy -->
            <RowDefinition Height="320"/>
            <!-- kody kreskowe -->
            <RowDefinition Height="109"/>
            <!-- panel dolny -->
            <RowDefinition Height="45"/>
        </Grid.RowDefinitions>
        <Canvas Grid.Row="0">

        </Canvas>
        <DataGrid 
            x:Name="dgWyroby" 
            Grid.Row="1" 
            AutoGenerateColumns="False" 
            ItemsSource="{Binding}" 
            RowDetailsVisibilityMode="VisibleWhenSelected" 
            SelectionChanged="DgWyroby_SelectionChanged"
            IsReadOnly="true" Margin="0,0,0,0" >

            <DataGrid.Columns>
                <DataGridTextColumn 
                    x:Name="idColumn"
                    Binding="{Binding id}" 
                    Header="id" 
                    Width="SizeToHeader" 
                    Visibility="Hidden"/>
                <DataGridTextColumn 
                    x:Name="wyrob_kodColumn" 
                    Binding="{Binding wyrob_kod}" 
                    Header="Kod" 
                    Width="100"/>
                <DataGridTextColumn 
                    x:Name="wyrob_kod_indeksColumn" 
                    Binding="{Binding wyrob_kod_indeks}" 
                    Header="Indeks" 
                    Width="100"/>
                <DataGridTextColumn 
                    x:Name="wyrob_kod_opisColumn" 
                    Binding="{Binding wyrob_kod_opis}" 
                    Header="Opis" 
                    Width="*"/>
                <DataGridTextColumn 
                    x:Name="wyrob_typColumn" 
                    Binding="{Binding wyrob_typ}" 
                    Header="Typ" 
                    Width="SizeToHeader"/>
                <DataGridComboBoxColumn
                    Header="Typ"
                    Width="100"
                    SelectedValuePath="indeks"
                    DisplayMemberPath="parametr"
                    ItemsSource="{Binding listTypyWyrobow}"
                    />
