        <DockPanel Grid.Row="2" HorizontalAlignment="Left" Width="600" Margin="0,5">
            <Button x:Name="btnDodaj" Content="DODAJ" Width="100" Height="30" FontSize="14" Click="BtnDodaj_Click" />
            <Button x:Name="btnKlonuj" Content="KLONUJ" Width="100" Height="30" FontSize="14" Click="BtnKlonuj_Click" />
            <Button x:Name="btnPopraw" Content="POPRAW" Width="100" Height="30" FontSize="14" Click="BtnPopraw_Click" />
            <Button x:Name="btnUsun" Content="USUŃ" Width="100" Height="30" FontSize="14" Foreground="Red"/>
            <Button x:Name="btnAnuluj" Content="ANULUJ" Width="100" Height="30" FontSize="14" Foreground="Red" Click="BtnAnuluj_Click"/>
            <Button x:Name="btnZatwierdz" Content="ZATWIERDŹ" Width="100" Height="30" FontSize="14" Foreground="Green" Click="BtnZatwierdz_Click" VerticalAlignment="Top"/>
        </DockPanel>
        <Grid x:Name="gridWyrob" ShowGridLines="False"  Grid.Row="3" Margin="0,0,0,0" >
            <Grid.ColumnDefinitions>
                <!-- Wyrób identyfikacja -->
                <ColumnDefinition Width="380" />
                <!-- ITF  -->
                <ColumnDefinition Width="380"/>
                <!-- Traceability -->
                <ColumnDefinition Width="380"/>
                <!-- inne -->
                <ColumnDefinition Width="380"/>
                <!-- puste miejsce -->
                <ColumnDefinition Width="*"/>
               
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
          
            </Grid.RowDefinitions>
            <!-- dwfinicja wyrobu -->
            <Grid Grid.Row="0" Grid.Column="0" x:Name="grdWyrob">
