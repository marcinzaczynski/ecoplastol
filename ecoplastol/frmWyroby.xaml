<Window x:Class="ecoplastol.frmWyroby"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ecoplastol"
        xmlns:ts="clr-namespace:ecoplastol"
        mc:Ignorable="d"
        Title="frmWyroby" Height="665" Width="1600" WindowState="Maximized" Loaded="Window_Loaded">
    <Window.Resources>
        <CollectionViewSource x:Key="wyrobyViewSource" d:DesignSource="{d:DesignInstance {x:Type local:wyroby}, CreateList=True}"/>

        <!-- TUTAJ DEFINIUJE SIĘ STYLE -->

        <!-- Converters -->

    </Window.Resources>

    <Window.CommandBindings>
        <CommandBinding Command="ApplicationCommands.Help" CanExecute="Sprawdz" Executed="Wykonaj"/>
    </Window.CommandBindings>
    <Window.InputBindings>
        <KeyBinding Command="ApplicationCommands.Help" Key="F2"/>

    </Window.InputBindings>
    <Grid HorizontalAlignment="Stretch" Height="auto" VerticalAlignment="Stretch" Width="auto" DataContext="{StaticResource wyrobyViewSource}" Margin="0,-6,0,5">
