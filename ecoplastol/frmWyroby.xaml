                    <!-- Wyrób identyfikacja - opisy -->
                    <ColumnDefinition Width="180" />
                    <!-- Wyrób identyfikacja - właściwości -->
                    <ColumnDefinition Width="200"/>
                </Grid.ColumnDefinitions>
                <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" BorderThickness="1" BorderBrush="black" Background="LightGray"/>
                <Label Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" FontSize="14" FontWeight="DemiBold" HorizontalContentAlignment="Center" Padding="0,3" Content="DEFINICJA WYROBU"/>
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="1" Grid.Column="0" Padding="10,0,0,0" Content="Typ :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="2" Grid.Column="0" Padding="10,0,0,0" Content="Kod :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="3" Grid.Column="0" Padding="10,0,0,0" Content="Indeks :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="4" Grid.Column="0" Padding="10,0,0,0" Content="Opis :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="5" Grid.Column="0" Padding="10,0,0,0" Content="Ilość w opak. zb :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="6" Grid.Column="0" Padding="10,0,0,0" Content="Waga opakowania :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="7" Grid.Column="0" Padding="10,0,0,0" Content="Waga kształtki :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="8" Grid.Column="0" Padding="10,0,0,0" Content="Zakres SDR :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="9" Grid.Column="0" Padding="10,0,0,0" Content="Norma na 1 zmianę :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="10" Grid.Column="0" Padding="10,0,0,0" Content="Ilość w partii :" />


                <ComboBox x:Name="cbbWyrobTyp" Grid.Row="1" Grid.Column="1"  Width="200" FontSize="14">
                    <ComboBox.ItemTemplateSelector>
                        <ts:ComboBoxItemTemplateSelector>
                            <ts:ComboBoxItemTemplateSelector.SelectedTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding wartosc}" />
                                </DataTemplate>
                            </ts:ComboBoxItemTemplateSelector.SelectedTemplate>
                            <ts:ComboBoxItemTemplateSelector.DropDownTemplate>
                                <DataTemplate>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Text="{Binding parametr}" Width="13"/>
                                        <TextBlock Text="-  "/>
                                        <TextBlock Text="{Binding wartosc}" />
                                    </StackPanel>
                                </DataTemplate>
                            </ts:ComboBoxItemTemplateSelector.DropDownTemplate>
                        </ts:ComboBoxItemTemplateSelector>
                    </ComboBox.ItemTemplateSelector>
                </ComboBox>
                <TextBox x:Name="txtWyrobKod" Grid.Row="2" Grid.Column="1" Text="" FontSize="14"/>
                <TextBox x:Name="txtWyrobIndeks" Grid.Row="3" Grid.Column="1" Text="" FontSize="14"/>
                <TextBox x:Name="txtWyrobOpis" Grid.Row="4" Grid.Column="1" Text="" FontSize="14"/>
                <TextBox x:Name="txtWyrobIwOpZ" Grid.Row="5" Grid.Column="1" Text="" FontSize="14" MaxLength="4" PreviewTextInput="TxtWyrobIwOpZ_PreviewTextInput"/>
                <TextBox x:Name="txtWyrobWagaOp" Grid.Row="6" Grid.Column="1" Text="" FontSize="14" MaxLength="4" PreviewTextInput="TxtWyrobWagaOp_PreviewTextInput"/>
                <TextBox x:Name="txtWyrobWaga1szt" Grid.Row="7" Grid.Column="1" Text="" FontSize="14" MaxLength="5" PreviewTextInput="TxtWyrobWaga1szt_PreviewTextInput"/>
                <ComboBox x:Name="cbbWyrobZakresSDR" Grid.Row="8" Grid.Column="1" FontSize="14">
                    <ComboBox.ItemTemplateSelector>
                        <ts:ComboBoxItemTemplateSelector>
                            <ts:ComboBoxItemTemplateSelector.SelectedTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding wartosc}" />
                                </DataTemplate>
                            </ts:ComboBoxItemTemplateSelector.SelectedTemplate>
                            <ts:ComboBoxItemTemplateSelector.DropDownTemplate>
                                <DataTemplate>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Text="{Binding parametr}" Width="13"/>
                                        <TextBlock Text="-  "/>
                                        <TextBlock Text="{Binding wartosc}" />
                                    </StackPanel>
                                </DataTemplate>
                            </ts:ComboBoxItemTemplateSelector.DropDownTemplate>
                        </ts:ComboBoxItemTemplateSelector>
                    </ComboBox.ItemTemplateSelector>
                </ComboBox>
                <TextBox x:Name="txtWyrobNorma" Grid.Row="9" Grid.Column="1" Text="" FontSize="14" MaxLength="4" PreviewTextInput="TxtWyrobNorma_PreviewTextInput"/>
                <TextBox x:Name="txtWyrobIlwPartii" Grid.Row="10" Grid.Column="1" Text="" FontSize="14" MaxLength="5" PreviewTextInput="TxtWyrobIlwPartii_PreviewTextInput"/>
            </Grid>
            <!-- ITF -->
            <Grid Grid.Row="0" Grid.Column="1" x:Name="grdITF">
                <Grid.RowDefinitions>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
