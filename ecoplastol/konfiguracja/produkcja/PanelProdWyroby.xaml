<UserControl x:Class="ecoplastol.konfiguracja.produkcja.PanelProdWyroby"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ecoplastol.konfiguracja.produkcja"
             xmlns:globals="clr-namespace:ecoplastol.globals"
             xmlns:ts="clr-namespace:ecoplastol"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="1600">
    <UserControl.Resources>
        <ResourceDictionary>
                <globals:BackgroundColor x:Key="valueToBackground"/>
                <globals:RezystancjaConverter x:Key="rezConverter"/>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid Background="White">
        <Grid.RowDefinitions>
            <RowDefinition Height="60"/>
            <!-- lista datagrid -->
            <RowDefinition Height="*"/>
            <!-- textboxy comboboxy -->
            <RowDefinition Height="320"/>
            <!-- kody kreskowe -->
            <RowDefinition Height="109"/>
            <!-- przyciski dodaj, popraw itd.-->
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="25"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="150"/>
                <ColumnDefinition Width="150"/>
                <ColumnDefinition Width="150"/>
                <ColumnDefinition Width="150"/>
                <ColumnDefinition Width="150"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="150"/>
                <ColumnDefinition Width="150"/>
            </Grid.ColumnDefinitions>
            <Button Grid.Row="0" Grid.Column="0" Content="ELEKTROOPOROWE" Name="btnPokazElektro" FontSize="14" FontWeight="DemiBold" Margin="5,2,5,2" Click="btnPokazElektro_Click"/>
            <Button Grid.Row="0" Grid.Column="1" Content="DOCZOŁOWE" Name="btnPokazDoczolowe" FontSize="14" FontWeight="DemiBold" Margin="5,2,5,2" Click="btnPokazDoczolowe_Click"/>
            <Button Grid.Row="0" Grid.Column="2" Content="ZAWORY" Name="btnPokazZawory" FontSize="14" FontWeight="DemiBold" Margin="5,2,5,2" Click="btnPokazZawory_Click"/>
            <Button Grid.Row="0" Grid.Column="3" Content="ADAPTERY" Name="btnPokazAdaptery" FontSize="14" FontWeight="DemiBold" Margin="5,2,5,2" Click="btnPokazAdaptery_Click"/>
            <Button Grid.Row="0" Grid.Column="4" Content="WSZYSTKIE" Name="btnPokazWszystkieKsztaltki" FontSize="14" FontWeight="DemiBold" Margin="5,2,5,2" Click="btnPokazWszystkieKsztaltki_Click" Foreground="Blue"/>
            <Button Grid.Row="0" Grid.Column="6" Content="WSZYSTKIE" Name="btnPokazWszystkie" FontSize="14" FontWeight="DemiBold" Margin="5,2,5,2" Click="BtnPokazWszystkie_Click"/>
            <Button Grid.Row="0" Grid.Column="7" Content="AKTYWNE" Name="btnPokazAktywne" FontSize="14" FontWeight="DemiBold" Margin="5,2,5,2" Click="BtnPokazAktywne_Click" Foreground="Blue"/>
            <Label Grid.Row="1" Grid.Column="6" Content="Iość pozycji :" Padding="0,0,5,0" VerticalContentAlignment="Center" HorizontalAlignment="Right"/>
            <Label Grid.Row="1" Grid.Column="7" Content="..." Padding="5,0,0,0" VerticalContentAlignment="Center" HorizontalAlignment="Left" Name="lblIloscPozycji"/>
        </Grid>
        <DataGrid x:Name="grdLista" Grid.Row="1" AutoGenerateColumns="False" ItemsSource="{Binding}" RowDetailsVisibilityMode="VisibleWhenSelected" SelectionChanged="GrdLista_SelectionChanged" IsReadOnly="true" Margin="0,0,0,0" >

            <DataGrid.Columns>
                <DataGridTextColumn x:Name="idColumn" Binding="{Binding id}"  Header="id" Width="SizeToHeader" Visibility="Hidden"/>
                <DataGridTextColumn x:Name="wyrob_kodColumn" Binding="{Binding wyrob_kod}" Header="Kod" Width="100"/>
                <DataGridTextColumn x:Name="wyrob_kod_indeksColumn" Binding="{Binding wyrob_kod_indeks}" Header="Indeks" Width="100"/>
                <!--<DataGridTextColumn x:Name="wyrob_kod_opisColumn" Binding="{Binding wyrob_kod_opis}" Header="Opis" Width="*"/>
                <DataGridTextColumn x:Name="wyrob_typColumn" Binding="{Binding wyrob_typ}" Header="Typ" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="wyrob_il_w_op_zbColumn" Binding="{Binding wyrob_il_w_op_zb}" Header="wyrob il w op zb" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="wyrob_waga_opColumn" Binding="{Binding wyrob_waga_op}" Header="wyrob waga op" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="wyrob_waga_1sztColumn" Binding="{Binding wyrob_waga_1szt}" Header="wyrob waga 1 szt" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="wyrob_zakres_sdrColumn" Binding="{Binding wyrob_zakres_sdr}" Header="wyrob zakres sdr" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="wyrob_zast_zaworuColumn" Binding="{Binding wyrob_zast_zaworu}" Header="wyrob zast zaworu" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="wyrob_rodzaj_drutuColumn" Binding="{Binding wyrob_rodzaj_drutu}" Header="wyrob rodzaj drutu" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="itf_kategoriaColumn" Binding="{Binding itf_kategoria}" Header="itf kategoria" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="itf_znak1Column" Binding="{Binding itf_znak1}" Header="itf znak 1" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="itf_znak2Column" Binding="{Binding itf_znak2}" Header="itf znak 2" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="itf_iccColumn" Binding="{Binding itf_icc}" Header="itf icc" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="itf_cc1Column" Binding="{Binding itf_cc1}" Header="itf cc 1" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="itf_cc2Column" Binding="{Binding itf_cc2}" Header="itf cc 2" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="itf_sminColumn" Binding="{Binding itf_smin}" Header="itf smin" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="itf_smaxColumn" Binding="{Binding itf_smax}" Header="itf smax" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="itf_trnColumn" Binding="{Binding itf_trn}" Header="itf trn" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="itf_prnColumn" Binding="{Binding itf_prn}" Header="itf prn" Width="SizeToHeader"/>-->
                <DataGridTextColumn x:Name="itf_rezColumn" Binding="{Binding itf_rez}" Header="Rezystancja" Width="SizeToHeader"/>
                <!--<DataGridTextColumn x:Name="itf_odchColumn" Binding="{Binding itf_odch}" Header="itf odch" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="itf_cz1Column" Binding="{Binding itf_cz1}" Header="itf cz 1" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="itf_cz2Column" Binding="{Binding itf_cz2}" Header="itf cz 2" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="itf_keColumn" Binding="{Binding itf_ke}" Header="itf ke" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="trace_znak1Column" Binding="{Binding trace_znak1}" Header="trace znak 1" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="trace_znak2Column" Binding="{Binding trace_znak2}" Header="trace znak 2" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="trace_kategoriaColumn" Binding="{Binding trace_kategoria}" Header="trace kategoria" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="trace_sminColumn" Binding="{Binding trace_smin}" Header="trace smin" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="trace_smaxColumn" Binding="{Binding trace_smax}" Header="trace smax" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="trace_partiaColumn" Binding="{Binding trace_partia}" Header="trace partia" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="trace_producentColumn" Binding="{Binding trace_producent}" Header="trace producent" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="trace_sdrColumn" Binding="{Binding trace_sdr}" Header="trace sdr" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="trace_pe_mColumn" Binding="{Binding trace_pe_m}" Header="trace pe m" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="trace_materialColumn" Binding="{Binding trace_material}" Header="trace material" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="trace_pe_oColumn" Binding="{Binding trace_pe_o}" Header="trace pe o" Width="SizeToHeader"/>
                <DataGridTextColumn x:Name="trace_mfrColumn" Binding="{Binding trace_mfr}" Header="trace mfr" Width="SizeToHeader"/>-->
                <DataGridTextColumn x:Name="opwColumn" Binding="{Binding opw}" Header="opw" Width="60"/>
                <DataGridTextColumn x:Name="czaswColumn" Binding="{Binding czasw, StringFormat=\{0:yyyy-MM-dd HH:mm:ss\}}" Header="czasw" Width="100"/>
                <DataGridTextColumn x:Name="opmColumn" Binding="{Binding opm}" Header="opm" Width="60"/>
                <DataGridTextColumn x:Name="czasmColumn" Binding="{Binding czasm, StringFormat=\{0:yyyy-MM-dd HH:mm:ss\}}" Header="czasm" Width="100"/>
                <!--<DataGridTextColumn x:Name="rezColumn" Binding="{Binding rez}" Header="rez TEST" Width="SizeToHeader"/>-->
            </DataGrid.Columns>
        </DataGrid>
        <Grid x:Name="grdPozycje" ShowGridLines="False"  Grid.Row="2" Margin="0,0,0,0" >
            <Grid.ColumnDefinitions>
                <!-- Wyrób identyfikacja -->
                <ColumnDefinition Width="380" />
                <!-- ITF  -->
                <ColumnDefinition Width="380"/>
                <!-- Traceability -->
                <ColumnDefinition Width="380"/>
                <!-- inne -->
                <ColumnDefinition Width="380"/>
                <!-- puste miejsce -->
                <ColumnDefinition Width="*"/>

            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>

            </Grid.RowDefinitions>
            <!-- dwfinicja wyrobu -->
            <Grid Grid.Row="0" Grid.Column="0" x:Name="grdWyrob">
                <Grid.RowDefinitions>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <!-- Wyrób identyfikacja - opisy -->
                    <ColumnDefinition Width="180" />
                    <!-- Wyrób identyfikacja - właściwości -->
                    <ColumnDefinition Width="200"/>
                </Grid.ColumnDefinitions>
                <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" BorderThickness="1" BorderBrush="black" Background="LightGray"/>
                <Label Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" FontSize="14" FontWeight="DemiBold" HorizontalContentAlignment="Center" Padding="0,3" Content="DEFINICJA WYROBU"/>
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="1" Grid.Column="0" Padding="10,0,0,0" Content="Typ :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="2" Grid.Column="0" Padding="10,0,0,0" Content="Kod :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="3" Grid.Column="0" Padding="10,0,0,0" Content="Indeks :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="4" Grid.Column="0" Padding="10,0,0,0" Content="Opis :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="5" Grid.Column="0" Padding="10,0,0,0" Content="Ilość w opak. zb :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="6" Grid.Column="0" Padding="10,0,0,0" Content="Waga opakowania :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="7" Grid.Column="0" Padding="10,0,0,0" Content="Waga kształtki :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="8" Grid.Column="0" Padding="10,0,0,0" Content="Zakres SDR :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="9" Grid.Column="0" Padding="10,0,0,0" Content="Norma na 1 zmianę :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="10" Grid.Column="0" Padding="10,0,0,0" Content="Ilość w partii :" />


                <ComboBox x:Name="cbbWyrobTyp" Grid.Row="1" Grid.Column="1"  Width="200" FontSize="14" SelectedValuePath="id" SelectedValue="{Binding wyrob_typ}">
                    <ComboBox.ItemTemplateSelector>
                        <ts:ComboBoxItemTemplateSelector>
                            <ts:ComboBoxItemTemplateSelector.SelectedTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding wartosc}" />
                                </DataTemplate>
                            </ts:ComboBoxItemTemplateSelector.SelectedTemplate>
                            <ts:ComboBoxItemTemplateSelector.DropDownTemplate>
                                <DataTemplate>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Text="{Binding parametr}" Width="13"/>
                                        <TextBlock Text="-  "/>
                                        <TextBlock Text="{Binding wartosc}" />
                                    </StackPanel>
                                </DataTemplate>
                            </ts:ComboBoxItemTemplateSelector.DropDownTemplate>
                        </ts:ComboBoxItemTemplateSelector>
                    </ComboBox.ItemTemplateSelector>
                </ComboBox>
                <TextBox Grid.Row="2" Grid.Column="1" FontSize="14" x:Name="txtWyrobKod" Text="{Binding wyrob_kod}" />
                <TextBox Grid.Row="3" Grid.Column="1" FontSize="14" x:Name="txtWyrobIndeks" Text="{Binding wyrob_kod_indeks}" />
                <TextBox Grid.Row="4" Grid.Column="1" FontSize="14" x:Name="txtWyrobOpis" Text="{Binding wyrob_kod_opis}"/>
                <TextBox Grid.Row="5" Grid.Column="1" FontSize="14" x:Name="txtWyrobIwOpZ" Text="{Binding wyrob_il_w_op_zb}" MaxLength="4" PreviewTextInput="TxtWyrobIwOpZ_PreviewTextInput"/>
                <TextBox Grid.Row="6" Grid.Column="1" FontSize="14" x:Name="txtWyrobWagaOp" Text="{Binding wyrob_waga_op}" MaxLength="4" PreviewTextInput="TxtWyrobWagaOp_PreviewTextInput"/>
                <TextBox Grid.Row="7" Grid.Column="1" FontSize="14" x:Name="txtWyrobWaga1szt" Text="{Binding wyrob_waga_1szt}" MaxLength="5" PreviewTextInput="TxtWyrobWaga1szt_PreviewTextInput"/>
                <ComboBox x:Name="cbbWyrobZakresSDR" Grid.Row="8" Grid.Column="1" FontSize="14" SelectedValuePath="id" SelectedValue="{Binding wyrob_zakres_sdr}">
                    <ComboBox.ItemTemplateSelector>
                        <ts:ComboBoxItemTemplateSelector>
                            <ts:ComboBoxItemTemplateSelector.SelectedTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding wartosc}" />
                                </DataTemplate>
                            </ts:ComboBoxItemTemplateSelector.SelectedTemplate>
                            <ts:ComboBoxItemTemplateSelector.DropDownTemplate>
                                <DataTemplate>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Text="{Binding parametr}" Width="13"/>
                                        <TextBlock Text="-  "/>
                                        <TextBlock Text="{Binding wartosc}" />
                                    </StackPanel>
                                </DataTemplate>
                            </ts:ComboBoxItemTemplateSelector.DropDownTemplate>
                        </ts:ComboBoxItemTemplateSelector>
                    </ComboBox.ItemTemplateSelector>
                </ComboBox>
                <TextBox Grid.Row="9" Grid.Column="1" FontSize="14" x:Name="txtWyrobNorma" Text="{Binding wyrob_norma}" MaxLength="4" PreviewTextInput="TxtWyrobNorma_PreviewTextInput"/>
                <TextBox Grid.Row="10" Grid.Column="1" FontSize="14" x:Name="txtWyrobIlwPartii" Text="{Binding wyrob_il_w_partii}" MaxLength="5" PreviewTextInput="TxtWyrobIlwPartii_PreviewTextInput"/>
            </Grid>
            <!-- ITF -->
            <Grid Grid.Row="0" Grid.Column="1" x:Name="grdITF">
                <Grid.RowDefinitions>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                    <RowDefinition Height="25"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <!-- Wyrób identyfikacja - opisy -->
                    <ColumnDefinition Width="180" />
                    <!-- Wyrób identyfikacja - właściwości -->
                    <ColumnDefinition Width="200"/>
                </Grid.ColumnDefinitions>
                <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" BorderThickness="1" BorderBrush="black" Background="LightGray"/>
                <Label Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" FontSize="14" FontWeight="DemiBold" HorizontalContentAlignment="Center" Padding="0,3" Content="PARAMETRY KODU ITF"/>
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="1" Grid.Column="0" Padding="10,0,0,0" Content="Kategoria :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="2" Grid.Column="0" Padding="10,0,0,0" Content="Znak 1 | 2 :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="3" Grid.Column="0" Padding="10,0,0,0" Content="Indykacja czasu chł. :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="4" Grid.Column="0" Padding="10,0,0,0" Content="Czas chłodzenia 1 | 2 :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="5" Grid.Column="0" Padding="10,0,0,0" Content="Średnica min | max :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="6" Grid.Column="0" Padding="10,0,0,0" Content="Typ regulacji napięcia :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="7" Grid.Column="0" Padding="10,0,0,0" Content="Poziom regulacji napięcia :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="8" Grid.Column="0" Padding="10,0,0,0" Content="Rezystancja :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="9" Grid.Column="0" Padding="10,0,0,0" Content="Odchylenia :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="10" Grid.Column="0" Padding="10,0,0,0" Content="Czas zgrzewania 1 | 2 :" />
                <Label VerticalAlignment="Center" FontSize="14" Grid.Row="11" Grid.Column="0" Padding="10,0,0,0" Content="Korekcja energii :" />
                <ComboBox x:Name="cbbITFKategoria" Grid.Row="1" Grid.Column="1" FontSize="14" DropDownClosed="CbbITFKategoria_DropDownClosed" GotFocus="CbbITFKategoria_GotFocus" LostFocus="CbbITFKategoria_LostFocus" SelectedValuePath="id" SelectedValue="{Binding itf_kategoria}">
                    <ComboBox.ItemTemplateSelector>
                        <ts:ComboBoxItemTemplateSelector>
                            <ts:ComboBoxItemTemplateSelector.SelectedTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding opis}" />
                                </DataTemplate>
                            </ts:ComboBoxItemTemplateSelector.SelectedTemplate>
                            <ts:ComboBoxItemTemplateSelector.DropDownTemplate>
                                <DataTemplate>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Text="{Binding wartosc}" Width="20"/>
                                        <TextBlock Text="-  "/>
                                        <TextBlock Text="{Binding opis}" Width="210"/>
                                    </StackPanel>
                                </DataTemplate>
                            </ts:ComboBoxItemTemplateSelector.DropDownTemplate>
                        </ts:ComboBoxItemTemplateSelector>
                    </ComboBox.ItemTemplateSelector>
                </ComboBox>
                <DockPanel Grid.Row="2" Grid.Column="1">
                    <ComboBox x:Name="cbbITFZnak1" Width="100" FontSize="14" DropDownClosed="CbbITFZnak1_DropDownClosed" GotFocus="CbbITFZnak1_GotFocus" LostFocus="CbbITFZnak1_LostFocus" SelectedValuePath="id" SelectedValue="{Binding itf_znak1}">
                        <ComboBox.ItemTemplateSelector>
                            <ts:ComboBoxItemTemplateSelector>
                                <ts:ComboBoxItemTemplateSelector.SelectedTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding wartosc}" />
                                    </DataTemplate>
                                </ts:ComboBoxItemTemplateSelector.SelectedTemplate>
                                <ts:ComboBoxItemTemplateSelector.DropDownTemplate>
                                    <DataTemplate>
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock Text="{Binding parametr}" Width="20"/>
                                            <TextBlock Text="-  "/>
                                            <TextBlock Text="{Binding wartosc}"/>
                                        </StackPanel>
                                    </DataTemplate>
                                </ts:ComboBoxItemTemplateSelector.DropDownTemplate>
                            </ts:ComboBoxItemTemplateSelector>
                        </ComboBox.ItemTemplateSelector>
                    </ComboBox>
                    <ComboBox x:Name="cbbITFZnak2" Width="100" FontSize="14" DropDownClosed="CbbITFZnak2_DropDownClosed" GotFocus="CbbITFZnak2_GotFocus" LostFocus="CbbITFZnak2_LostFocus" SelectedValuePath="id" SelectedValue="{Binding itf_znak2}">
                        <ComboBox.ItemTemplateSelector>
                            <ts:ComboBoxItemTemplateSelector>
                                <ts:ComboBoxItemTemplateSelector.SelectedTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding wartosc}" />
                                    </DataTemplate>
                                </ts:ComboBoxItemTemplateSelector.SelectedTemplate>
                                <ts:ComboBoxItemTemplateSelector.DropDownTemplate>
                                    <DataTemplate>
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock Text="{Binding parametr}" Width="20"/>
                                            <TextBlock Text="-  "/>
                                            <TextBlock Text="{Binding wartosc}"/>
                                        </StackPanel>
                                    </DataTemplate>
                                </ts:ComboBoxItemTemplateSelector.DropDownTemplate>
                            </ts:ComboBoxItemTemplateSelector>
                        </ComboBox.ItemTemplateSelector>
                    </ComboBox>
                </DockPanel>
                <ComboBox x:Name="cbbITFICC" Grid.Row="3" Grid.Column="1" FontSize="14" DropDownClosed="CbbITFICC_DropDownClosed" GotFocus="CbbITFICC_GotFocus" LostFocus="CbbITFICC_LostFocus" SelectedValuePath="id" SelectedValue="{Binding itf_icc}">
                    <ComboBox.ItemTemplateSelector>
                        <ts:ComboBoxItemTemplateSelector>
                            <ts:ComboBoxItemTemplateSelector.SelectedTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding wartosc}" />
                                </DataTemplate>
                            </ts:ComboBoxItemTemplateSelector.SelectedTemplate>
                            <ts:ComboBoxItemTemplateSelector.DropDownTemplate>
                                <DataTemplate>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Text="{Binding parametr}" Width="20"/>
                                        <TextBlock Text="-  "/>
                                        <TextBlock Text="{Binding wartosc}"/>
                                    </StackPanel>
                                </DataTemplate>
                            </ts:ComboBoxItemTemplateSelector.DropDownTemplate>
                        </ts:ComboBoxItemTemplateSelector>
                    </ComboBox.ItemTemplateSelector>
                </ComboBox>
                <DockPanel Grid.Row="4" Grid.Column="1">
                    <ComboBox x:Name="cbbITFCC1" FontSize="14" Width="100" DropDownClosed="CbbITFCC1_DropDownClosed" GotFocus="CbbITFCC1_GotFocus" LostFocus="CbbITFCC1_LostFocus" SelectedValuePath="id" SelectedValue="{Binding itf_cc1}">
                        <ComboBox.ItemTemplateSelector>
                            <ts:ComboBoxItemTemplateSelector>
                                <ts:ComboBoxItemTemplateSelector.SelectedTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding wartosc}" />
                                    </DataTemplate>
                                </ts:ComboBoxItemTemplateSelector.SelectedTemplate>
                                <ts:ComboBoxItemTemplateSelector.DropDownTemplate>
                                    <DataTemplate>
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock Text="{Binding parametr}" Width="20"/>
                                            <TextBlock Text="-  "/>
                                            <TextBlock Text="{Binding wartosc}"/>
                                        </StackPanel>
                                    </DataTemplate>
                                </ts:ComboBoxItemTemplateSelector.DropDownTemplate>
                            </ts:ComboBoxItemTemplateSelector>
                        </ComboBox.ItemTemplateSelector>
                    </ComboBox>
                    <ComboBox x:Name="cbbITFCC2" FontSize="14" DropDownClosed="CbbITFCC2_DropDownClosed" GotFocus="CbbITFCC2_GotFocus" LostFocus="CbbITFCC2_LostFocus" SelectedValuePath="id" SelectedValue="{Binding itf_cc2}">
                        <ComboBox.ItemTemplateSelector>
                            <ts:ComboBoxItemTemplateSelector>
                                <ts:ComboBoxItemTemplateSelector.SelectedTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding wartosc}" />
                                    </DataTemplate>
                                </ts:ComboBoxItemTemplateSelector.SelectedTemplate>
                                <ts:ComboBoxItemTemplateSelector.DropDownTemplate>
                                    <DataTemplate>
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock Text="{Binding parametr}" Width="20"/>
                                            <TextBlock Text="-  "/>
                                            <TextBlock Text="{Binding wartosc}"/>
                                        </StackPanel>
                                    </DataTemplate>
                                </ts:ComboBoxItemTemplateSelector.DropDownTemplate>
                            </ts:ComboBoxItemTemplateSelector>
                        </ComboBox.ItemTemplateSelector>
                    </ComboBox>
                </DockPanel>
                <DockPanel Grid.Row="5" Grid.Column="1">
                    <ComboBox x:Name="cbbITFsmin" Width="100" FontSize="14" DropDownClosed="CbbITFsmin_DropDownClosed" GotFocus="CbbITFsmin_GotFocus" LostFocus="CbbITFsmin_LostFocus" SelectedValuePath="id" SelectedValue="{Binding itf_smin}">
                        <ComboBox.ItemTemplateSelector>
                            <ts:ComboBoxItemTemplateSelector>
                                <ts:ComboBoxItemTemplateSelector.SelectedTemplate>
                                    <DataTemplate>
